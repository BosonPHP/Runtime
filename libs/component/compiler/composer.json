{
    "name": "boson-php/compiler",
    "description": "Boson compiler",
    "type": "library",
    "license": "MIT",
    "homepage": "https://bosonphp.com",
    "support": {
        "issues": "https://github.com/boson-php/boson/issues",
        "source": "https://github.com/boson-php/runtime"
    },
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "thecutcode@gmail.com"
        },
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "bin": ["boson-compiler"],
    "require": {
        "php": "^8.4",
        "symfony/console": "^7.0",
        "bamarni/composer-bin-plugin": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Boson\\Component\\Compiler\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "bin-compat": "full",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
